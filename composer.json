{
    "name": "yiisoft/yii2-app-basic",
    "description": "Yii 2 Basic Project Template",
    "keywords": ["yii2", "plugins", "basic", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "minimum-stability": "stable",
    "require":
    {
        "php": ">=7.2.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "yiisoft/yii2-imagine": "^2.2",
        "sizeg/yii2-jwt": "v2.0.1.1",
        "shmilyzxt/yii2-queue": "dev-dev",
        "linslin/yii2-curl": "*",
        "overtrue/easy-sms": "^1.3",
        "aliyuncs/oss-sdk-php": "v2.3.0",
        "qiniu/php-sdk": "7.2.7",
        "guzzlehttp/guzzle": "6.5.5",
        "qcloud/cos-sdk-v5": "^2.2",
        "wechatpay/wechatpay": "^1.4"
    },
    "require-dev":
    {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "codeception/specify": "~0.4.6",
        "symfony/browser-kit": ">=2.7 <=4.2.4",
        "codeception/module-filesystem": "^1.0.0",
        "codeception/module-yii2": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "config":
    {
        "process-timeout": 1800,
        "fxp-asset":
        {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "autoload":
    {
        "psr-4":
        {
            "workbox\\": "./workbox/",
            "framework\\": "./system/",
            "plugins\\": "./plugins/"
        },
        "files": [
            "system/functions.php",
            "system/phpqrcode/qrlib.php"
        ]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer"
        }
    }
}
